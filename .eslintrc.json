{
  "extends": [
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "plugin:jsx-a11y/recommended"
    // "plugin:prettier/recommended",
    // "prettier/react"
  ],
  "plugins": [
    "jsx-a11y"
    // "prettier"
  ],
  "rules": {
    // "prettier/prettier": ["error", { "singleQuote": true }],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/no-onchange": 0,
    "no-console": 0,
    "no-param-reassign": 0
  }
}